{
  "name": "Murmur",
  "version": "0.1",
  "image": "byteri/hassio-addons-murmur-{arch}",
  "slug": "murmur",
  "description": "Murmur server",
  "url": "https://github.com/selleronom/hassio-addons/tree/master/murmur",
  "startup": "services",
  "arch": ["armhf", "armv7", "aarch64"],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "host_dbus": true,
  "ports": {
    "64738/tcp": 64738,
    "64738/udp": 64738
  },
  "ports_description": {
    "64738/tcp": "Murmur TCP server port",
    "64738/udp": "Murmur UDP server port"
  },
  "map": ["ssl"],
  "options": {
    "registerName": "Hass.io",
    "serverpassword": "Hass.io",
    "bandwidth": "130000",
    "ssl": true,
    "certfile": "/ssl/fullchain.pem",
    "keyfile": "/ssl/privkey.pem"
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)?",
    "registerName": "str",
    "serverpassword": "str",
    "bandwidth": "str",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str"
  }
}
